{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint", 
    "prettier"
  ],
  "rules": {
      "prettier/prettier": [
          "error"
      ],
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
  }
}
